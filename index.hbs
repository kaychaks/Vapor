{{!< default}}

<main class="content" role="main">



    {{#foreach posts}}

    <article class="preview">
        <header>
		{{#has tag="linkedlist"}}                        
			<h1 class="post-title linkedlistpost"><a href="{{url}}">{{meta_title}}</a></h1>
                 {{else}}                        
			<h1 class="post-title"><a href="{{url}}">{{title}}</a></h1>
                 {{/has}}

            <!--<div class="post-meta"><time datetime="{{date format='YYYY-MM-DD'}}">{{date format="dddd, MMMM DD, YYYY"}}</time></div>-->
            <div class="post-meta"><time class="post-date" datetime="{{date format="YYYY-MM-DDTHH:mm:ss.SS\Z"}}">...</time></div>
        </header>
        <section class="post-excerpt">
            <p>{{excerpt}}&hellip;</p>
            <p class="readmore"><a href="{{url}}">Read this article <i class="fa fa-chevron-circle-right"></i></a></p>
        </section>
    </article>



    {{/foreach}}


    {{pagination}}

</main>
